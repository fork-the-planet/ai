{
	"compilerOptions": {
		"target": "es2021",
		"lib": ["es2021", "DOM", "DOM.Iterable"],
		"jsx": "react-jsx",
		"module": "es2022",
		"moduleResolution": "Bundler",
		"resolveJsonModule": true,
		"allowJs": true,
		"noEmit": true,
		"isolatedModules": true,
		"strict": true,
		"skipLibCheck": true,
		"types": ["@cloudflare/workers-types/experimental"]
	},
	"include": ["src/**/*.ts", "src/**/*.tsx"]
}
