{
	"name": "@cloudflare/tanstack-ai",
	"type": "module",
	"version": "0.0.0",
	"description": "Use TanStack AI with Cloudflare Workers AI and AI Gateway",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./adapters/openai": {
			"types": "./dist/adapters/openai.d.ts",
			"import": "./dist/adapters/openai.js",
			"require": "./dist/adapters/openai.cjs"
		},
		"./adapters/anthropic": {
			"types": "./dist/adapters/anthropic.d.ts",
			"import": "./dist/adapters/anthropic.js",
			"require": "./dist/adapters/anthropic.cjs"
		},
		"./adapters/gemini": {
			"types": "./dist/adapters/gemini.d.ts",
			"import": "./dist/adapters/gemini.js",
			"require": "./dist/adapters/gemini.cjs"
		},
		"./adapters/grok": {
			"types": "./dist/adapters/grok.d.ts",
			"import": "./dist/adapters/grok.js",
			"require": "./dist/adapters/grok.cjs"
		},
		"./adapters/workers-ai": {
			"types": "./dist/adapters/workers-ai.d.ts",
			"import": "./dist/adapters/workers-ai.js",
			"require": "./dist/adapters/workers-ai.cjs"
		}
	},
	"files": [
		"dist",
		"README.md"
	],
	"keywords": [
		"cloudflare",
		"workers",
		"ai",
		"tanstack",
		"gateway",
		"chat",
		"serverless"
	],
	"author": "Cloudflare Inc.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/cloudflare/ai.git"
	},
	"bugs": {
		"url": "https://github.com/cloudflare/ai/issues"
	},
	"scripts": {
		"build": "rm -rf dist && tsup --config tsup.config.ts",
		"format": "biome format --write",
		"type-check": "tsc --noEmit",
		"test": "vitest",
		"test:ci": "vitest --watch=false",
		"test:e2e": "vitest --config vitest.e2e.config.ts --watch=false",
		"test:e2e:rest": "vitest --config vitest.e2e.config.ts --watch=false test/e2e/workers-ai-rest.e2e.test.ts",
		"test:e2e:binding": "vitest --config vitest.e2e.config.ts --watch=false test/e2e/workers-ai-binding.e2e.test.ts"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "^7.56.3",
		"@tanstack/ai": "^0.4.2",
		"dotenv": "^17.2.3"
	},
	"dependencies": {
		"openai": "^6.16.0"
	},
	"peerDependencies": {
		"@tanstack/ai": "^0.4.2"
	},
	"optionalDependencies": {
		"@anthropic-ai/sdk": "^0.71.2",
		"@cloudflare/workers-types": "^4.20260128.0",
		"@google/genai": "^1.35.0",
		"@tanstack/ai-anthropic": "^0.4.2",
		"@tanstack/ai-gemini": "^0.4.1",
		"@tanstack/ai-grok": "^0.4.1",
		"@tanstack/ai-openai": "^0.4.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "pnpm@10.26.2"
}
